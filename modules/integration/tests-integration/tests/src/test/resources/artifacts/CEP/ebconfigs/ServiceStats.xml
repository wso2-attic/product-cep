<?xml version="1.0" encoding="UTF-8"?>
<eventBuilder name="ServiceStats" statistics="disable" trace="enable"
              xmlns="http://wso2.org/carbon/eventbuilder">
    <from eventAdaptorName="WSO2EventAdaptor" eventAdaptorType="wso2event">
        <property name="stream">org.wso2.sample.service.data</property>
        <property name="version">1.0.0</property>
    </from>
    <mapping type="wso2event" customMapping="disable"/>
    <to streamName="org.wso2.sample.service.data" version="1.0.0"/>
</eventBuilder>