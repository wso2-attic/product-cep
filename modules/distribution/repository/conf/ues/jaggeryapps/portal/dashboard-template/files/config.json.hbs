{
    "server": {
        "https": "{{httpsServerUrl}}",
        "http": "{{httpServerUrl}}"
    },
    "roles": {
        "portal": {
            "/permission/admin/login": ["ui.execute"]
        }
    },
    "tenantConfigs": "/_system/config/{{appName}}/configs/{{appName}}.json",
    "adminRole": "admin",
    "user": {
        "username": "admin",
        "password": "admin"
    },
    "userRoles": ["portal"],
    "permissions": {
        "login": {
            "/permission/admin/login": ["ui.execute"]
        }
    },
    "userSpace": {
        "space": "{{appName}}",
        "options": {
            "type": "registry",
            "serverUrl": "{{httpsServerUrl}}/admin",
            "path": "/_system/governance/users",
            "username": "admin",
            "password": "admin"
        }
    },
    "storeSpace": {
        "space": "store",
        "options": {
            "type": "registry",
            "serverUrl": "{{httpsServerUrl}}/admin",
            "path": "/_system/governance/users",
            "username": "admin",
            "password": "admin"
        }
    },
    "ssoConfiguration": {
        "enabled": true,
        "issuer": "{{appName}}",
        "identityProviderURL": "{{httpsServerUrl}}/sso/samlsso.jag",
        "keyStorePassword": "wso2carbon",
        "identityAlias": "wso2carbon",
        "responseSigningEnabled": "true",
        "keyStoreName": "/repository/resources/security/wso2carbon.jks",
        "appAcs" : "{{httpsServerUrl}}/{{appName}}/sso.jag",
    }
}