<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>Data Source Handler</servlet-name>
		<servlet-class>org.wso2.carbon.http.servlet.objectdetector.DataSourceHandler</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Image Display</servlet-name>
		<servlet-class>org.wso2.carbon.http.servlet.objectdetector.ImageDisplay</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Data Source Handler</servlet-name>
		<url-pattern>/imageshow</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Image Display</servlet-name>
		<url-pattern>/displayImage</url-pattern>
	</servlet-mapping>
	
</web-app>
